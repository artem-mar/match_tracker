{
  "printWidth": 80,
  "trailingComma": "es5",
  "arrowParens": "avoid",
  "semi": false,
  "bracketSpacing": true,
  "jsxSingleQuote": true,
  "singleQuote": true,
  "jsxBracketSameLine": false,
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true,
  "plugins": ["prettier-plugin-tailwindcss"]
}
